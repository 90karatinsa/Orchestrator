# Zorunlu
GITHUB_OWNER=owner
GITHUB_REPO=repo
BASE_BRANCH=main

# PR ve sürücü stratejileri
PR_STRATEGY=codex
CODEX_DRIVER=ui

# Codex UI
CODEX_UI_URL=https://chatgpt.com/codex
CODEX_UI_COOKIES_JSON=./cookies.json
CODEX_UI_EMAIL=
CODEX_UI_PASSWORD=

# Çalışma parametreleri
TASKS_FILE=tasks/todo.md
PR_EVERY=3
BATCH_SIZE=2
UI_POLL_SEC=60
GLOBAL_TIMEOUT_SEC=900

# Selectors
SEL_REPO_TOGGLE=.min-w-fit > div:nth-child(1) > div:nth-child(1) > div:nth-child(1) > div:nth-child(1) > div:nth-child(1) > button:nth-child(1)
SEL_CODE_BUTTON=.btn-primary
SEL_BRANCH_MENU=div.rounded-full:nth-child(2) > button:nth-child(1)

# Opsiyonel QA
QA_ENABLED=false
QA_BUILD_CMD=pnpm build
QA_TEST_CMD=pnpm test -i --silent
QA_LINT_CMD=pnpm lint

# Opsiyonel duraklatma
PAUSE=false
